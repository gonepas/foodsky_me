<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container", id="sml1">
    <%= image_tag("xt_logo.png", alt: "Facebook link", class: 'navbar-header') %>
    <%= link_to "Foodsky by SkyTeam", root_path, id: "logo" %>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to "Home", root_path, id: "sml1" %></li>
        <li><%= link_to "Help", help_path, id: "sml1" %></li>
        <% if logged_in? %>
          <li><%= link_to "Users", users_path, id: "sml1"  %></li>
          <li class="dropdown" >
            <a href="#" class="dropdown-toggle" data-toggle="dropdown"  id="sml1" >
              Account <b class="caret"></b>
            </a>
            <ul class="dropdown-menu">
              <li><%= link_to "Profile", current_user, id: "sml1" %></li>
              <li><%= link_to "Settings", edit_user_path(current_user), id: "sml1"  %></li>
              <li class="divider"></li>
              <li>
                <%= link_to "Log out", logout_path, method: "delete", id: "sml1"  %>
              </li>
            </ul>
          </li>
        <% else %>
          <li><%= link_to "Log in", login_path, id: "sml1"  %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>